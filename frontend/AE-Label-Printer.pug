doctype html
html(lang="en")
  head
    meta(charset="utf-8")
    meta(name="viewport", content="width=device-width, initial-scale=1, shrink-to-fit=no")
    meta(name="description", content="")
    meta(name="author", content="")
    link(rel="icon", href="./favicon.ico")
    title AE Labels
    link(rel="canonical", href="https://getbootstrap.com/docs/4.0/examples/navbar-fixed/")
    // Bootstrap core CSS
    link(href="./bootstrap-page-files/bootstrap.min.css", rel="stylesheet")
    // Custom styles for this template
    link(href="./bootstrap-page-files/navbar-top-fixed.css", rel="stylesheet")
    link(href="./bootstrap-page-files/system-hardware-page.css", rel="stylesheet")
    link(href="./AE-Label-Printer/AE-Label-Printer.css", rel="stylesheet")
  body
    //-nav.navbar.navbar-expand-md.navbar-dark.fixed-top.bg-dark
      a.navbar-brand(href="#") PAR
      button.navbar-toggler(type="button", data-toggle="collapse", data-target="#navbarCollapse", aria-controls="navbarCollapse", aria-expanded="false", aria-label="Toggle navigation")
        span.navbar-toggler-icon
      .collapse.navbar-collapse#navbarCollapse
        ul.nav-item.mr-auto
        //-ul.navbar-nav.mr-auto
          li.nav-item.active
            a.nav-link(href="ProductInfo") Home 
              span.sr-only (current)
          li.nav-item
            a.nav-link(href="/WarrantyInfo.html") Warranty
          li.nav-item
            a.nav-link(href="#") Support
          li.nav-item
            a.nav-link.disabled(href="#") Disabled
        form.form-inline.mt-2.mt-md-0
          input.form-control.mr-sm-2#serial-search-box(type="text", placeholder="Search", aria-label="Search")
          button.btn.btn-outline-success.my-2.my-sm-0#serial-search-btn(type="button") Search
    main(role="main", class="container")
      .jumbotron
        h1 AE Label Printing
        p.lead Enter a product model and serial into the fields below. Then choose from the label formatting options provided.
        //-.d-flex
          a.btn.btn-lg.btn-primary(href="https://support.partech.com/documentation/", role="button") View support docs »
          a.btn.btn-lg.btn-danger.ml-auto(href="https://support.partech.com/documentation/", role="button") Export list to csv »
      .log-request-container
        //-h3
          p
            br
          | Labels will be generated based on the below information.
          hr
        h4
          form#label-request
            .label-request-form
              label Model Number: 
                input(type="text")
            .label-request-form
              label Serial Number: 
                input(type="text")
        ul#printButtons.list-unstyled.m-0.p-0
          .row#cards-list
            li.col-md-3.mb-3.sample
              //- use only 1280 by 840 images for best consistency with card sizes
              .card
                img.card-img-top(alt="Card image cap" src="./Model-Serial.jpg")
                .card-body
                  h5.card-title System Model
                  p.card-text
                    | Serial
                  a.btn.btn-sm.btn-danger.btn-danger-light.label-selector-button(role="button") 1801 Model Serial Label
            li.col-md-3.mb-3.sample
              .card
                img.card-img-top(alt="Card image cap" src="https://support.partech.com/wp-content/uploads/2023/11/PAR-Logo-NB-blue-65492e1d8e588.svg")
                .card-body
                  h5.card-title System Model
                  p.card-text
                    | Serial
                  a.btn.btn-sm.btn-danger.btn-danger-light.label-selector-button(role="button") Box Model 1801 Label
        #pdf-Iframe
    // Bootstrap core JavaScript
    // Placed at the end of the document so the pages load faster
    script(src="./bootstrap-page-files/jquery-3.2.1.slim.min.js", integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN", crossorigin="anonymous")
    script.
      window.jQuery || document.write('<script src="./bootstrap-page-files/jquery-3.2.1.slim.min.js"><\/script>')
    script(src="./bootstrap-page-files/popper.min.js")
    script(src="./bootstrap-page-files/bootstrap.min.js")
    script(src="./AE-Label-Printer/AE-Label-Printer.js")
    script(src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js")